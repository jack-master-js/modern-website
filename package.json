{
  "name": "website",
  "version": "0.1.0",
  "scripts": {
    "reset": "rm -rf node_modules",
    "dev": "modern dev",
    "build": "modern build",
    "start": "modern start",
    "new": "modern new",
    "lint": "modern lint",
    "apidoc": "apidoc -i api/ -o apidoc"
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "eslintConfig": {
    "extends": [
      "@modern-js"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet",
      "git add"
    ],
    "*.{js,jsx,mjs,mjsx,cjs,cjsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet",
      "git add"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@modern-js/plugin-bff": "^1.0.0",
    "@modern-js/plugin-express": "^1.0.0",
    "@modern-js/plugin-less": "1.0.0",
    "@modern-js/runtime": "^1.0.0",
    "express": "^4.17.1",
    "log4js": "^6.3.0",
    "md5": "^2.3.0",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@modern-js/app-tools": "^1.0.0",
    "@modern-js/plugin-jarvis": "^1.0.0",
    "@types/express": "^4.17.13"
  },
  "modernConfig": {
    "runtime": {
      "router": true,
      "state": true
    }
  },
  "apidoc": {
    "name": "modern-website",
    "description": "API document",
    "version": "0.1.0",
    "title": "Node Api Document",
    "sampleUrl": "http://localhost:8080"
  }
}
